generateheaders: $(GENERATEDHEADERS)
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) generateheaders) || exit 1; done

clean:
	rm -f $(TARGET)$(EXE_SUFFIX)
	rm -f *.o *_n.cc *_n.h *_m.cc *_m.h .tstamp
	rm -f *.vec *.sca
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) clean); done

depend:
	$(MAKEDEPEND) $(INCLUDE_PATH) -- *.cc
	# $(MAKEDEPEND) $(INCLUDE_PATH) -fMakefile.in -- *.cc
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) depend) || exit 1; done

makefiles:
	# recreate Makefile
	opp_makemake -f  -x -u Cmdenv -b /home/kristjan/mobility-fw -c /home/kristjan/mobility-fw/omnetppconfig -lxml2 -I/usr/include/libxml2 -I/home/kristjan/mobility-fw/core/include -I/home/kristjan/mobility-fw/contrib/include -L/home/kristjan/mobility-fw/core/lib -lmfcore -L/home/kristjan/mobility-fw/contrib/lib -lmfcontrib 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefiles) || exit 1; done

makefile-ins:
	# recreate Makefile.in
	opp_makemake -f -m  -x -u Cmdenv -b /home/kristjan/mobility-fw -c /home/kristjan/mobility-fw/omnetppconfig -lxml2 -I/usr/include/libxml2 -I/home/kristjan/mobility-fw/core/include -I/home/kristjan/mobility-fw/contrib/include -L/home/kristjan/mobility-fw/core/lib -lmfcore -L/home/kristjan/mobility-fw/contrib/lib -lmfcontrib 
	for i in $(SUBDIRS); do (cd $$i && $(MAKE) makefile-ins) || exit 1; done

# "re-makemake" and "re-makemake-m" are deprecated, historic names of the above two targets
re-makemake: makefiles
re-makemake-m: makefile-ins


